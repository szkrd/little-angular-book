{
  "name": "little-angular-book",
  "version": "2.0.0",
  "description": "Getting started with Angular 5+",
  "main": "index.js",
  "scripts": {
    "build": "node src/build.js",
    "build:watch": "nodemon -w book -w overlay -w src --ext js,md,ejs,css src/build.js",
    "serve": "node src/serve.js",
    "dev": "concurrently npm:build:watch npm:serve",
    "format": "prettier --write src book"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szkrd/little-angular-book.git"
  },
  "keywords": [],
  "author": "Szabolcs Kurdi <szabolcs.kurdi@gmail.com> (https://github.com/szkrd)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/szkrd/little-angular-book/issues"
  },
  "homepage": "https://github.com/szkrd/little-angular-book#readme",
  "devDependencies": {
    "concurrently": "5.2.0",
    "ejs": "3.1.9",
    "emojilib": "3.0.10",
    "express": "4.18.2",
    "highlight.js": "10.1.1",
    "marked": "4.3.0",
    "nodemon": "2.0.22",
    "prettier": "2.0.5",
    "shelljs": "0.8.5",
    "unicode-emoji-json": "0.4.0"
  }
}
